{
  "name": "sequant",
  "version": "1.14.1",
  "description": "Quantize your development workflow - Sequential AI phases with quality gates",
  "type": "module",
  "bin": {
    "sequant": "dist/bin/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "templates",
    "stacks",
    ".claude-plugin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx bin/cli.ts",
    "test": "vitest run",
    "lint": "eslint src/ bin/ --max-warnings 0",
    "sync:skills": "cp -r templates/skills/* .claude/skills/",
    "validate:skills": "for skill in templates/skills/*/; do npx skills-ref validate \"$skill\"; done",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "workflow",
    "ai",
    "automation",
    "quality",
    "sequential",
    "agent-skills",
    "cursor",
    "vscode"
  ],
  "author": "Sequant Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/admarble/sequant.git"
  },
  "bugs": {
    "url": "https://github.com/admarble/sequant/issues"
  },
  "homepage": "https://github.com/admarble/sequant#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.11",
    "@hono/node-server": "^1.19.9",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chokidar": "^5.0.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "diff": "^7.0.0",
    "gradient-string": "^3.0.0",
    "hono": "^4.11.4",
    "inquirer": "^12.3.2",
    "open": "^11.0.0",
    "ora": "^8.2.0",
    "yaml": "^2.7.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/diff": "^7.0.0",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.52.0",
    "vitest": "^3.2.4"
  }
}
