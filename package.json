{
  "name": "sequant",
  "version": "0.1.0",
  "description": "Quantize your development workflow - Sequential AI phases with quality gates",
  "type": "module",
  "bin": {
    "sequant": "./dist/bin/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "templates",
    "stacks"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx bin/cli.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src bin --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "workflow",
    "ai",
    "automation",
    "quality",
    "sequential",
    "agent-skills",
    "cursor",
    "vscode"
  ],
  "author": "Matcha Maps <hello@getmatchamaps.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/admarble/sequant.git"
  },
  "bugs": {
    "url": "https://github.com/admarble/sequant/issues"
  },
  "homepage": "https://github.com/admarble/sequant#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "diff": "^7.0.0",
    "inquirer": "^12.3.2",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/diff": "^7.0.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
